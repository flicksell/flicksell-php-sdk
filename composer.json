{
    "name": "flicksell/auth-sdk",
    "description": "Official Flicksell Authentication SDK for secure app-to-store communication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "FlickSell Team",
            "email": "dev@flicksell.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FlickSell\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlickSell\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "flicksell",
        "authentication",
        "hmac",
        "api",
        "ecommerce"
    ]
} 