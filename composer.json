{
    "name": "flicksell/php-sdk",
    "description": "Official PHP SDK for FlickSell stores with OAuth 2.0 and HMAC authentication support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "FlickSell Team",
            "email": "dev@flicksell.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "predis/predis": "^2.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FlickSell\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlickSell\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "flicksell",
        "ecommerce",
        "api",
        "oauth",
        "hmac",
        "authentication"
    ],
    "homepage": "https://flicksell.com",
    "support": {
        "issues": "https://github.com/flicksell/php-sdk/issues",
        "docs": "https://docs.flicksell.com"
    }
} 